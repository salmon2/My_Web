doctype html
html
  head
    meta(charset='utf-8')
    title 시퀄라이즈 서버
    style.
      table {
        border: 1px solid black;
        border-collapse: collapse;
      }

      table th, table td {
        border: 1px solid black;
      }
  body
    table#user-list
      thead
        tr
          th ID
          th 이름
      tbody
        for user in users
          tr
            td= user.id
            td= user.name
    br